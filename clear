[32m+[m[32mfunction circleChaptaKaro() {[m
[32m+[m[32m  // define default scale value[m
[32m+[m[32m  var xscale = 1;[m
[32m+[m[32m  var yscale = 1;[m
[32m+[m
[32m+[m[32m  var xprev = 0;[m
[32m+[m[32m  var yprev = 0;[m
[32m+[m
[32m+[m[32m  window.addEventListener("mousemove", function (dets) {[m
[32m+[m[32m    clearTimeout(timeout);[m
[32m+[m
[32m+[m[32m    xscale = gsap.utils.clamp(0.8, 1.2, dets.clientX - xprev);[m
[32m+[m[32m    yscale = gsap.utils.clamp(0.8, 1.2, dets.clientY - yprev);[m
[32m+[m
[32m+[m[32m    xprev = dets.clientX;[m
[32m+[m[32m    yprev = dets.clientY;[m
[32m+[m
[32m+[m[32m    circleMouseFollower(xscale, yscale);[m
[32m+[m
[32m+[m[32m    timeout = setTimeout(function () {[m
[32m+[m[32m      document.querySelector([m
[32m+[m[32m        "#minicircle"[m
[32m+[m[32m      ).style.transform = `translate(${dets.clientX}px, ${dets.clientY}px) scale(1, 1)`;[m
[32m+[m[32m    }, 100);[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction circleMouseFollower(xscale, yscale) {[m
[32m+[m[32m  window.addEventListener("mousemove", function (dets) {[m
[32m+[m[32m    document.querySelector([m
[32m+[m[32m      "#minicircle"[m
