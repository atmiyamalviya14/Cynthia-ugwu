[32m+[m[32m// teeno element ko sleect karo, uske baad teeno par ek mousemove lagao, jab mousemove ho to ye pata karo ki mouse kaha par hai, jiska matlab hai mouse ki x and y position pata karo, ab mouse ki x y position ke badle us image ko show karo and us image ko move karo, move karte waqt rotate karo, and jaise jaise mouse tez chale waise waise rotation bhi tez ho jaye[m
[32m+[m
[32m+[m[32mdocument.querySelectorAll(".elem").forEach(function (elem) {[m
[32m+[m[32m  var rotate = 0;[m
[32m+[m[32m  var diffrot = 0;[m
[32m+[m
[32m+[m[32m  elem.addEventListener("mouseleave", function (dets) {[m
[32m+[m[32m    gsap.to(elem.querySelector("img"), {[m
[32m+[m[32m      opacity: 0,[m
[32m+[m[32m      ease: Power3,[m
[32m+[m[32m      duration: 0.5,[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  elem.addEventListener("mousemove", function (dets) {[m
[32m+[m[32m    var diff = dets.clientY - elem.getBoundingClientRect().top;[m
[32m+[m[32m    diffrot = dets.clientX - rotate;[m
[32m+[m[32m    rotate = dets.clientX;[m
[32m+[m[32m    gsap.to(elem.querySelector("img"), {[m
[32m+[m[32m      opacity: 1,[m
[32m+[m[32m      ease: Power3,[m
[32m+[m[32m      top: diff,[m
[32m+[m[32m      left: dets.clientX,[m
[32m+[m[32m      rotate: gsap.utils.clamp(-20, 20, diffrot * 0.5),[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
