[32m+[m[32mfunction firstPageAnim() {[m
[32m+[m[32m  var tl = gsap.timeline();[m
[32m+[m
[32m+[m[32m  tl.from("#nav", {[m
[32m+[m[32m    y: "-10",[m
[32m+[m[32m    opacity: 0,[m
[32m+[m[32m    duration: 1.5,[m
[32m+[m[32m    ease: Expo.easeInOut,[m
[32m+[m[32m  })[m
[32m+[m[32m    .to(".boundingelem", {[m
[32m+[m[32m      y: 0,[m
[32m+[m[32m      ease: Expo.easeInOut,[m
[32m+[m[32m      duration: 2,[m
[32m+[m[32m      delay: -1,[m
[32m+[m[32m      stagger: 0.2,[m
[32m+[m[32m    })[m
[32m+[m[32m    .from("#herofooter", {[m
[32m+[m[32m      y: -10,[m
[32m+[m[32m      opacity: 0,[m
[32m+[m[32m      duration: 1.5,[m
[32m+[m[32m      delay: -1,[m
[32m+[m[32m      ease: Expo.easeInOut,[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
